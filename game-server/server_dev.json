{
    "apps": [
        {
          "name": "server-chat",
          "script": "app.js",
          "instances": 1,
          "exec_mode": "fork",
          "merge_logs": true,
          "error_file": "/data/logs/server-chat-error.log",
          "out_file": "/data/logs/server-chat-out.log",
          "log_file": "/data/logs/server-chat-all.log",
          "combine_logs": false,
          "log_date_format": "YYYY-MM-DD HH:mm:ss",
          "env": {
            "NODE_ENV": "development",
            "TZ": "Asia/Shanghai"
          }
        }
    ]
}